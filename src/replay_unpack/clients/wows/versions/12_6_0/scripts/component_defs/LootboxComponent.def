<root>

	<BaseMethods>
		<openLootbox>
			<Exposed/>
			<Args>
				<lootboxType> GAMEPARAMS_ID </lootboxType>
			</Args>
		</openLootbox>

		<openLootboxesByType>
			<Exposed/>
			<Args>
				<lootboxType> GAMEPARAMS_ID </lootboxType>
				<amount> UINT16 </amount>
			</Args>
		</openLootboxesByType>

		<openLootboxes>
			<Exposed/>
		</openLootboxes>
	</BaseMethods>

	<ClientMethods>
		<onSingleLootboxOpened>
			<Args>
				<lootboxType> GAMEPARAMS_ID </lootboxType>
				<rewards> MSGPACK_BLOB </rewards>
			</Args>
			<VariableLengthHeaderSize>
				<WarnLevel>none</WarnLevel>
			</VariableLengthHeaderSize>
		</onSingleLootboxOpened>

		<onMassLootboxOpenFailed/>
		<onSingleLootboxOpenFailed/>

		<onOpenLootboxesStarted>
			<Args>
				<num> UINT16 </num>
			</Args>
		</onOpenLootboxesStarted>

		<updateOpenedLootboxCount>
			<Args>
				<num> UINT16 </num>
			</Args>
		</updateOpenedLootboxCount>

	</ClientMethods>

	<ofEntity>
		<Account/>
	</ofEntity>
</root>

